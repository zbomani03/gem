{
  "name": "@agstudio/agstudio",
  "version": "1.1.1",
  "private": true,
  "dependencies": {
    "@agstudio/services": "^1.1.1",
    "@agstudio/translations": "^1.1.1",
    "@agstudio/web": "^1.1.1",
    "@gkernel/ux-components": "1.25.2",
    "@reach/component-component": "^0.1.3",
    "classnames": "^2.2.6",
    "formik": "^1.5.8",
    "lodash": "^4.17.15",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-intl": "^3.2.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "PORT=3000 react-scripts start",
    "test": "react-scripts test --env=jsdom --coverage --coverageDirectory=coverage/report",
    "build:development": "env-cmd -f .env.development react-scripts build",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/gsap": "^1.20.2",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/mockdate": "^2.0.0",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-intl": "^3.0.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-test-renderer": "^16.9.0",
    "@types/webpack-env": "^1.14.0",
    "@types/yup": "^0.26.23",
    "env-cmd": "^10.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-plugin-react-hooks": "^2.0.1",
    "exports-loader": "^0.7.0",
    "fetch-mock": "^7.3.9",
    "imports-loader": "^0.8.0",
    "jest-enzyme": "^7.1.1",
    "jest-localstorage-mock": "^2.4.0",
    "mockdate": "^2.0.5",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.9.0",
    "typescript": "3.6.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/setupTests.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "coverageReporters": [
      "lcov"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
